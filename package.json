    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "bootstrap": "^5.2.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^9.0.0",
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "ngx-extended-pdf-viewer": "^16.0.2",
