    "@angular/material": "^17.3.8",
    "@angular/platform-browser": "^17.3.8",
    "@angular/platform-browser-dynamic": "^17.3.8",
    "@angular/router": "^17.3.8",
    "@angular/service-worker": "^17.3.8",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@sendgrid/mail": "^8.1.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "ngx-extended-pdf-viewer": "^20.2.0",
    "nodemailer": "^6.9.13",
