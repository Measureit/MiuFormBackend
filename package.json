{
  "name": "miucheck",
  "version": "1.0.0",
  "author": "CodiCraft",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "ng": "ng",
    "serveFront": "ng serve",
    "builddev": "ng build -c development && tsc -p server",
    "build": "ng build && npm run bundleServer && cp package.json dist/",
    "start": "node server/app.js",
    "bundleServer": "esbuild server/app.ts --platform=node --bundle --outfile=dist/server/app.js",
    "startBundleServer": "node dist/server/app.js",
    "startdev": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"ng serve --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"ng build && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint && htmlhint \"client/**/*.html\" && sass-lint \"client/**/*.scss\" -v",
    "lintfix": "ng lint --fix",
    "buildAAB": "bubblewrap build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.8",
    "@angular/cdk": "^17.3.8",
